.skill{
    width: 155px;
    height: 155px;
    position: relative;
 
}

.skill__circle{
    fill: transparent;

    &-gray{
        stroke: #e9e9e9;
        stroke-width: 10px;
    }

    &-gradient{
        stroke-width: 20px;
        stroke-dasharray: 243;
        stroke-dashoffset: 282;
    }
    
}

.skill__circle-title{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    text-align: center;
}